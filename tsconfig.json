{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "es2015", // Ubah dari 'commonjs' ke 'es2015'
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": ["api/**/*.ts"],
  "exclude": ["node_modules"]
}
