{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "module": "es2015", 
    "moduleResolution": "node", // Menambahkan ini untuk resolusi modul
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["api/**/*.ts", "custom.d.ts"],
  "exclude": ["node_modules"]
}
